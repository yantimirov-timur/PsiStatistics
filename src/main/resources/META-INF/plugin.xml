<idea-plugin>
    <id>org.example.TestPlugin</id>
    <name>PsiStatistis</name>
    <vendor>Yantimirov-Timur</vendor>

    <description>Plugin collect PSi elements statistics in file </description>
    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="PsiStatistics" class="PsiStatistics" text="ShowStat">
            <keyboard-shortcut keymap="$default" first-keystroke="ctrl Q" second-keystroke="ctrl U"/>
        </action>
    </actions>
</idea-plugin>